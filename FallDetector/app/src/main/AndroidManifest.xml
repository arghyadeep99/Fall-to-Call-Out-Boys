<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dimitris.falldetector">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_man_circle"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_man_circle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.SetActivity" />
        <activity android:name=".ui.StartActivity" />
        <activity android:name=".ui.HistoryActivity"></activity>
    </application>

</manifest>